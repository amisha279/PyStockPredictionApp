    "# ---------------------------------------------------------\n",
    "# 1. CONFIG\n",
    "# ---------------------------------------------------------\n",
    "\n",
    "TICKERS = [\n",
    "    \"AAPL\", \"TSLA\", \"MSFT\", \"GOOGL\", \"META\",\n",
    "    \"NVDA\", \"AMZN\", \"NFLX\", \"AMD\", \"INTC\",\n",
    "    \"ETH\", \"BTC\", \"Gold\", \"Silver\", \"RGTI\",\n",
    "    \"COST\", \"MU\", \"GOOG\", \"BTC\", \"DJI\", \"INX\",\n",
    "    \"T\", \"ONDS\"\n",
    "\n",
    "]\n",
    "\n",
    "YEARS = \"5y\"   # how much history to download\n",
    "INTERVAL = \"1d\""
   ],
   "id": "acd424e78dcccd7",
   "outputs": [],
   "execution_count": 9
  },
